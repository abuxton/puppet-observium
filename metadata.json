{
  "name": "abuxton-observium",
  "version": "0.1.0",
  "author": "abuxton",
  "summary": "a module to install Observium from http://www.observium.org/",
  "license": "Apache-2.0",
  "source": "https://github.com/abuxton/puppet-observium.git",
  "project_page": "https://github.com/abuxton/puppet-observium",
  "issues_url": "https://github.com/abuxton/puppet-observium/issues",
  "dependencies": [
      { "name":"puppetlabs/stdlib","version_requirement":">= 3.2.0 < 5.0.0"},
      { "name":"puppet/staging","version_requirement":">= 1.0.1 < 3.0.0"},
      { "name":"puppet/php","version_requirement":">= 4.0.0"},
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    }
  ]
}
